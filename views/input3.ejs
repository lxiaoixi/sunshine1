<body data-type="index">
	<!-- topbar start //-->
    <%- include('admin/portal/header') %>
	<!-- topbar end //-->
    <div class="am-cf admin-main">
    			<!-- side nav bar start //-->
        <%- include('admin/portal/sidernav') %>
			<!-- side nav bar end //-->


<div class="admin-content">
    <div class="admin-content-body">
      <div class="am-cf am-padding am-padding-bottom-0">
        <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">稿件管理</strong> / <small>新增街道联络员名单</small></div>
      </div>

      <hr/>

      

        <div class="am-u-sm-12 am-u-md-12 am-u-md-pull-12">
          <form class="am-form am-form-horizontal">
            <div class="am-form-group">
              <label for="user-name" class="am-u-sm-3 am-form-label">街道办名称 / Name</label>
              <div class="am-u-sm-9">
                <input type="text" id="user-name" placeholder="请输入街道办名称">
              </div>
            </div>

        

            <div class="am-form-group">
              <label for="user-intro" class="am-u-sm-3 am-form-label">联络员名单 / Table</label>
              <div class="am-u-sm-9">
                <textarea class="" rows="5" id="user-intro" placeholder="请输入街道办联络员的名单"></textarea>
              </div>
            </div>

            <div class="am-form-group">
              <div class="am-u-sm-9 am-u-sm-push-3">
                <button type="button" class="am-btn am-btn-primary">提交</button>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>

    <footer class="admin-content-footer">
      <hr>
